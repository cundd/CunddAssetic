{
  "name": "cundd/assetic",
  "description": "Assetic for TYPO3 CMS",
  "keywords": [
    "assets",
    "compression",
    "minification",
    "typo3"
  ],
  "homepage": "https://github.com/cundd/CunddAssetic",
  "type": "typo3-cms-extension",
  "license": "MIT",
  "authors": [
    {
      "name": "Daniel Corn",
      "email": "info@cundd.net",
      "homepage": "https://cundd.net/"
    }
  ],
  "require": {
    "php": "^8.1",
    "ext-ctype": "*",
    "typo3/cms-core": "^12.4.6 || ^13.4.0",
    "cundd/assetic_additions": "^2.0.0",
    "assetic/framework": "^3.1.2"
  },
  "require-dev": {
    "cboden/ratchet": "^0.4.3"
  },
  "autoload": {
    "psr-4": {
      "Cundd\\Assetic\\": "Classes/"
    }
  },
  "suggest": {
    "leafo/lessphp": "Allows to compile LESS with PHP",
    "leafo/scssphp": "Allows to compile SCSS 3.x with PHP",
    "cboden/ratchet": "For livereload support (0.4.x-dev)"
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "assetic"
    },
    "cundd/composer": {
      "install": false
    }
  }
}
